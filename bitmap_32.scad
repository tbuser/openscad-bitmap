font_column_bits=31;

module bitmap(bitmap, block_size, height, column_bits, row_bits) {
	x_length = block_size * row_bits;
	y_length = block_size * column_bits;
	bitmap_size = row_bits * column_bits;

	function loc_y(loc) = floor(loc / row_bits) * block_size;
	function loc_x(loc) = loc % row_bits * block_size;
	function loc_z(loc) = (bitmap[loc]*height-height)/2;

	translate(v = [-x_length/2+block_size/2,-y_length/2+block_size/2,height/2]) {
		for (loc = [0:bitmap_size - 1]) {
			if (bitmap[loc] != 0) {
				union() {
					translate(v = [loc_x(loc), loc_y(loc), loc_z(loc)]) {
						cube(size = [block_size+0.01, block_size+0.01, height * bitmap[loc]], center = true);
						//echo(str("loc: ",loc," loc_x: ", loc_x(loc) , " loc_y: ",  loc_y(loc)));
					}
				}
			}
		}
	}
}

